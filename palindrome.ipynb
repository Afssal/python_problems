   "source": [
    "num=int(input(\"Enter the number\"))\n",
    "temp=num\n",
    "no=0\n",
    "while temp>0:\n",
    "    digit=temp%10\n",
    "    no=no*10+digit\n",
    "    temp=temp//10\n",
    "if no==num:\n",
    "    print(\"palindrome\")\n",
    "else:\n",
    "    print(\"not palindrome\")"
   ]
